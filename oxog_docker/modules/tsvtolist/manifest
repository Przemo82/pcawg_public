name=TsvToList
description=takes Firehose tsv file (as generated by members.* or children.*) and converts it into gatk-style input bam file list, gatk-style input-output filename map or picard-style options file (I=...)
commandLine=perl <libdir>tsv_to_list.pl --tsv <tsv.file> --basename <base.name> <map.modifier> <list.extension> <map.extension> <picard.extension> <flags>
fileFormat=list,options,map

p1_name=tsv.file
p1_description=file containing list of tab-separated <id> <value> pairs or a single bam file
p1_default_value=
p1_fileFormat=
p1_optional=
p1_prefix_when_specified=
p1_MODE=IN
p1_TYPE=FILE
p1_type=java.io.File
p1_value=

p2_name=base.name
p2_description=base name for output file(s)
p2_default_value=
p2_fileFormat=
p2_optional=
p2_prefix_when_specified=
p2_MODE=
p2_TYPE=
p2_type=java.lang.String
p2_value=

p3_name=map.modifier
p3_description=required if --gatk_map is on; specifies extension to add to the id field in order to generate output file name in the map
p3_default_value=
p3_fileFormat=
p3_optional=on
p3_prefix_when_specified=--map_modifier 
p3_MODE=
p3_TYPE=
p3_type=java.lang.String
p3_value=

p4_name=list.extension
p4_description=extension for gatk-style list file to be created (list of bams to process); no list if this option is not given
p4_default_value=
p4_fileFormat=
p4_optional=on
p4_prefix_when_specified=--gatk_bam_list 
p4_MODE=
p4_TYPE=
p4_type=java.lang.String
p4_value=

p5_name=map.extension
p5_description=extension for gatk-style input-output map file to be created (input bam->output bam); no map if this option is not given
p5_default_value=
p5_fileFormat=
p5_optional=on
p5_prefix_when_specified=--gatk_map 
p5_MODE=
p5_TYPE=
p5_type=java.lang.String
p5_value=

p6_name=picard.extension
p6_description=extension for picard-style options file (I=input.bam for all input files); not generated if this option is not given
p6_default_value=
p6_fileFormat=
p6_optional=on
p6_prefix_when_specified=--picard_options 
p6_MODE=
p6_TYPE=
p6_type=java.lang.String
p6_value=

p7_name=flags
p7_description=optional command line flags
p7_default_value=
p7_fileFormat=
p7_optional=on
p7_prefix_when_specified=
p7_MODE=
p7_TYPE=
p7_type=java.lang.String
p7_value=

